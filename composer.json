{
    "name": "brokeyourbike/remit-one-api-client",
    "description": "RemitONE API Client for PHP",
    "type": "library",
    "license": "MPL-2.0",
    "keywords": [
        "api",
        "api-client",
        "r1",
        "remitone",
        "remit-one",
        "money-transfer"
    ],
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\RemitOne\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\RemitOne\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "brokeyourbike/http-client": "^1.0",
        "brokeyourbike/resolve-uri": "^1.0",
        "brokeyourbike/http-enums": "^2.0",
        "brokeyourbike/has-source-model": "^2.0",
        "symfony/serializer": "^6.0.3",
        "symfony/property-access": "^6.0.3"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "nesbot/carbon": "^2.55"
    },
    "version": "0.5.4",
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
